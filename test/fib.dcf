// Print Fibonacci series

class FibonacciSeries
{
    void main ()
    {
        int N, prev, curr, next;
        prev = 0;
        curr = 1;
        next = 1;
        N = 20;

        callout("printf", "Fibonacci Series upto %d numbers: ", N);
        for cnt=0, N
        {
            callout("printf", "%d ", curr);
            prev = curr;
            curr = next;
            next = prev + curr;
        }
    }
}
