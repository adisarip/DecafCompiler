// Print Fibonacci series

class FibonacciSeries
{
    void main ()
    {
        int N, prevValue, currValue, nextValue;
        prevValue = 0;
        currValue = 1;
        nextValue = 0;
        N = 5;

        callout("printf", "Enter a positive integer: ");
        //callout("printf", "N", "M");
        callout("printf", "%d  %d  ", prevValue, currValue); // print the starting value

        for count=1, N-2
        {
            nextValue = prevValue + currValue;
            callout("printf", "%d  ", nextValue);
            prevValue = currValue;
            currValue = nextValue;
            count = count + 1;
        }
        //callout("printf", "\n");
    }
}
